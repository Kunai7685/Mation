local TheMationsUI = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local UIGradient = Instance.new("UIGradient")
local UICorner = Instance.new("UICorner")
local Title = Instance.new("TextLabel")
local UICorner_2 = Instance.new("UICorner")
local UIGradient_2 = Instance.new("UIGradient")
local Pattern = Instance.new("ImageLabel")
local UICorner_3 = Instance.new("UICorner")
local ScrollingFrame = Instance.new("ScrollingFrame")
local UIListLayout = Instance.new("UIListLayout")
local Exit = Instance.new("TextButton")
local UICorner_4 = Instance.new("UICorner")

TheMationsUI.Name = "TheMationsUI"
TheMationsUI.Parent = game.CoreGui
TheMationsUI.DisplayOrder = 999999999

MainFrame.Name = "MainFrame"
MainFrame.Parent = TheMationsUI
MainFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
MainFrame.BorderColor3 = Color3.fromRGB(27, 42, 53)
MainFrame.BorderSizePixel = 0
MainFrame.Position = UDim2.new(0.378632486, 0, 0.345601887, 0)
MainFrame.Size = UDim2.new(0, 284, 0, 183)

UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(137, 0, 254)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(223, 0, 255))}
UIGradient.Parent = MainFrame

UICorner.Parent = MainFrame

Title.Name = "Title"
Title.Parent = MainFrame
Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Title.BorderSizePixel = 0
Title.Position = UDim2.new(-0.0176056344, 0, 0.0546448082, 0)
Title.Size = UDim2.new(0, 294, 0, 20)
Title.Font = Enum.Font.Unknown
Title.Text = "Mations"
Title.TextColor3 = Color3.fromRGB(146, 70, 161)
Title.TextScaled = true
Title.TextSize = 14.000
Title.TextWrapped = true

UICorner_2.Parent = Title

UIGradient_2.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(123, 0, 231)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(203, 0, 234))}
UIGradient_2.Parent = Title

Pattern.Name = "Pattern"
Pattern.Parent = MainFrame
Pattern.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Pattern.BackgroundTransparency = 1.000
Pattern.Position = UDim2.new(0, 0, -0.00273224036, 0)
Pattern.Size = UDim2.new(0, 284, 0, 183)
Pattern.Image = "rbxassetid://2151741365"
Pattern.ImageTransparency = 0.820
Pattern.ScaleType = Enum.ScaleType.Tile
Pattern.SliceCenter = Rect.new(0, 256, 0, 256)
Pattern.TileSize = UDim2.new(0, 250, 0, 250)

UICorner_3.Parent = Pattern

ScrollingFrame.Parent = MainFrame
ScrollingFrame.Active = true
ScrollingFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ScrollingFrame.BackgroundTransparency = 1.000
ScrollingFrame.BorderColor3 = Color3.fromRGB(27, 42, 53)
ScrollingFrame.BorderSizePixel = 0
ScrollingFrame.Position = UDim2.new(0.0316901393, 0, 0.224043712, 0)
ScrollingFrame.Size = UDim2.new(0, 266, 0, 133)
ScrollingFrame.ScrollBarThickness = 3

UIListLayout.Parent = ScrollingFrame
UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
UIListLayout.Padding = UDim.new(0, 3)

Exit.Name = "Exit"
Exit.Parent = MainFrame
Exit.BackgroundColor3 = Color3.fromRGB(113, 33, 140)
